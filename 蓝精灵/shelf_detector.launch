<?xml version="1.0"?>
<launch>
  <node name="shelf_detector" pkg="shelf_detector" type="shelf_detector.py" output="screen" >
     
   <param name="half_shelf_view" value="False" />

   <param name="consider_distance" value="5.0" />
   <param name="consider_range_front" value="3.0" />
   <param name="consider_range_back" value="-1.0" />
   <param name="consider_range_side" value="1.0" />
   
   <param name="shelf_size_informed" value="False" />
   <param name="shelf_front_rear" value="2.0" />
   <param name="shelf_left_right" value="1.3" />
   <param name="weight_size_match" value="20" />

   <param name="edge_tolerance" value="0.05" />
   <param name="right_angle_tolerance" value="0.08" />
   <param name="perpendicular_yaw_error" value="0.08" />
   <param name="horizontal_criterion" value="0.38" />
   <param name="radius_ratio_tolerance" value="0.0" />
   <param name="radius_diff_tolerance" value="0.3" />
   <param name="local_xy_error" value="0.1" />
   <param name="local_yaw_error" value="0.1" />
   
   <param name="object_timeout" value="30" />

   <param name="set_shelf_footprint" value="True" />
   <param name="footprint_rotation" value="False" />
   <param name="shelf_visualization" value="True" />

   <param name="median_filter_group" value="150" />
   <param name="median_filter_initial_group" value="20" />
   <param name="fifo" value="-1" />
   <param name="publish_rate" value="20" />
   
   <param name="global_xy_error" value="0.1" />
   <param name="global_yaw_error" value="0.1" />

   <param name="if_show_animation" value="False" />
   <param name="plot_pause_interval" value="20" />
   <param name="plot_sample_interval" value="3" />

   <param name="if_log" value="False" />
   <param name="log_path" value="/home/hitrobot/Downloads" />
  
  </node>
</launch>
